{
  "name": "rhondahud",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "electron": "electron electron/main.js",
    "server": "cross-env NODE_ENV=production node server/main.js",
    "build-vue": "vue-cli-service build",
    "build-electron": "electron-packager . RhondaHUD --platform=win32 --arch=ia32",
    "dev-electron": "cross-env NODE_ENV=development DEBUG=app:* nodemon --watch electron --watch config --exec yarn electron",
    "dev-server": "cross-env NODE_ENV=development DEBUG=app:* nodemon --watch server --watch config --ignore  server/collections --exec yarn server",
    "dev-vue": "cross-env NODE_ENV=development DEBUG=app:* yarn serve"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "config": "^3.1.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "electron": "^5.0.5",
    "express": "^4.17.1",
    "image-webpack-loader": "^5.0.0",
    "json5": "^2.1.0",
    "live-server": "^1.2.1",
    "nedb": "^1.8.0",
    "nedb-async": "^0.0.9",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "style-resources-loader": "^1.2.1",
    "tailwindcss": "^1.0.4",
    "uvs": "^0.1.3",
    "vue": "^2.6.10",
    "vue-native-websocket": "^2.0.13",
    "vue-router": "^3.0.3",
    "vue-socket.io": "^3.0.7",
    "vue2-circle-progress": "^1.2.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-service": "^3.8.0",
    "cross-env": "^5.2.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "nodemon": "^1.19.1",
    "vue-template-compiler": "^2.6.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
